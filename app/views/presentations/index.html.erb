<h1>Listing presentations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Client</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @presentations.each do |presentation| %>
  <tr>
    <td><%= presentation.name %></td>
    <td><%= presentation.description %></td>
    <td><%= presentation.client_id %></td>
    <td><%= link_to 'Show', client_presentation %></td>
    <td><%= link_to 'Edit', edit_client_presentation_path(presentation) %></td>
    <td><%= link_to 'Destroy', client_presentation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

